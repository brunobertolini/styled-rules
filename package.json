{
  "name": "styled-rules",
  "version": "0.1.0",
  "description": "Preset style rules, theme based, hich customizable style props",
  "license": "MIT",
  "repository": "brunobertolini/styled-rules",
  "author": {
    "name": "Bruno Bertolini",
    "email": "dev@brunobertolini.com",
    "url": "brunobertolini.com"
  },
  "engines": {
    "node": ">=4"
  },
  "main": "lib/index.js",
  "jsnext:main": "build/styled-rules.min.js",
  "module": "build/styled-rules.min.js",
  "scripts": {
    "build": "yarn build:lib && yarn build:build",
    "prebuild:lib": "rimraf lib/*",
    "build:lib": "babel --out-dir lib --ignore \"test.js\" src",
    "prebuild:build": "rimraf build/*",
    "build:build": "rollup -c ",
    "lint": "xo",
    "test": "xo && nyc ava",
    "precommit": "yarn lint",
    "prepush": "yarn run test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version --no-verify",
    "prepublish": "yarn build"
  },
  "files": [
    "build/",
    "lib/"
  ],
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styled-components",
    "props",
    "theme"
  ],
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codecov": "^3.0.0",
    "husky": "^0.14.3",
    "nyc": "^11.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "standard-version": "^4.2.0",
    "validate-commit-msg": "^2.14.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "rules": {
      "operator-linebreak": "off",
      "object-curly-spacing": "off",
      "space-infix-ops": "off"
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
