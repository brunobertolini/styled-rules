{
  "name": "styled-rules",
  "version": "0.3.0",
  "description": "Presets style rules, theme based, highly customizable style props",
  "license": "MIT",
  "repository": "brunobertolini/styled-rules",
  "author": {
    "name": "Bruno Bertolini",
    "email": "dev@brunobertolini.com",
    "url": "brunobertolini.com"
  },
  "main": "lib/index.js",
  "scripts": {
    "lint": "prettier --config ./.prettierrc --write '{src,test,.}/**/*.js'",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "build": "rollup -c ",
    "coverge": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "nyc ava",
    "precommit": "pretty-quick --staged",
    "prepush": "yarn run test",
    "release": "standard-version --no-verify",
    "prepublish": "yarn build"
  },
  "files": [
    "build/",
    "lib/"
  ],
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styled-components",
    "props",
    "theme"
  ],
  "engines": {
    "node": ">=8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "ava": "^0.25.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codecov": "^3.0.0",
    "husky": "^0.14.3",
    "nyc": "^11.0.0",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-uglify": "^3.0.0",
    "standard-version": "^4.2.0"
  }
}
